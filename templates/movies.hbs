{{#each movie}}
<section class="movie-holder" key="{{@key}}">
  <div>
    <img src="{{poster}}" alt="{{title}} poster">
  </div>
  {{#if seenit}}
  <div class="rating">stars go here</div>
  {{else}}
  <div>
    <button class="btn btn-success watched-btn">Watched</button>
  </div>
  {{/if}}
</section>
{{/each}}